{
    "title": "AndroidInterview",
    "author" : "yeungeek",
    "links" : {
      "sidebar" : {
          "Yeungeek" : "https://yeungeek.github.io/"
      }
    },
    "plugins" : [
      "anchor-navigation-ex",
      "expandable-chapters-interactive",
      "vistors-github",
      "mermaid-gb3",
      "github-issue-feedback",
      "github",
      "theme-comscore",
      "katex",
      "-highlight",
      "prism",
      "prism-themes",
      "flexible-alerts",
      "lightbox",
      "splitter"
    ],
    "pluginsConfig" : {
      "vistors-github": {
        "repo": "yeungeek/AndroidInterview"
      },
      "github-issue-feedback": {
          "repo": "yeungeek/AndroidInterview"
      },
      "anchor-navigation-ex": {
        "showLevel": false,
        "tocLevel1Icon": "fa fa-hand-o-right",
        "tocLevel2Icon": "fa fa-hand-o-right",
        "tocLevel3Icon": "fa fa-hand-o-right"
     },
     "github": {
      "url": "https://github.com/yeungeek/AndroidInterview"
     },
     "mathjax":{
      "forceSVG": true
    },
    "theme-default": {
      "showLevel": true
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-atom-dark.css"
      ]
    },
    "flexible-alerts": {
      "style": "flat",
      "comment": {
        "label": "Comment",
        "icon": "",
        "className": "tip",
        "labelVisibility":"hidden",
        "iconVisibility":"hidden"
      },
      "source": {
        "label": "",
        "icon": "fa fa-code",
        "className": "info",
        "labelVisibility":"hidden",
        "iconVisibility":"hidden"
      }
    }
  }
}